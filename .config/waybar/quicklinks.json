{
    "custom/quicklink1": {
        "format": " ",
        "on-click": "firefox",
        "tooltip-format": "Firefox"
    }, 
    "custom/quicklink2": {
        "format": " ",
        "on-click": "hyprctl dispatch exec [float] -- dolphin",
        "tooltip-format": "Dolphin"
    },
    "custom/quicklink3": {
        "format": "",
        "on-click": "firefox --new-tab https://www.youtube.com",
        "tooltip-format": "Youtube"
    }, 
    "custom/quicklink4": {
        "format": "",
        "on-click": "emacs",
        "tooltip-format": "Emacs"
    }, 
    "custom/quicklink5": {
        "format": "󱀣",
        "on-click": "firefox --private-window",
        "tooltip-format": "Private Browsing"
    }, 
    "custom/quicklink6": {
        "format": "",
        "on-click": "~/.config/hypr/toggle-spotify.sh",
        "tooltip-format": "Spotify"
    }, 
    "custom/quicklink7": {
        "format": "",
        "on-click": "betterbird",
        "tooltip-format": "Thunderbird"
    }, 
    "custom/quicklinkempty": {
    }, 
    "group/quicklinks": {
        "orientation": "horizontal",
        "modules": [
            "custom/quicklink1",
            "custom/quicklink2",
            "custom/quicklink3",
            "custom/quicklink4",
            "custom/quicklink5",
            "custom/quicklink6",
            "custom/quicklink7",
            "custom/quicklinkempty"
        ]
    }
}
